pipeline{
    agent{
        node{
            label "dockerSlave-jdk11"
        }
    }
    stages{
        stage("DockerStage"){
            steps{
                sh """
                echo "This is my first docker pipeline!"
                """
            }
        }
        stage("FINISH LAB"){
            steps{
                sh """
                echo "THIS IS OUR LAST EXAMPLE 4 DEEP DIVE JENKINS SESSION ^_^"
                """
            }
        }
    }
}
    